            "^MOOOV-special: (.*)$",
            "^Superhelden: (.*)$",
            "^(.*)\\s+\\(VL\\)$",
            "^HD-(.*)$"
        ],
        "YearSplitterPatterns": [
            "^(.*)\\s*\\((19\\d\\d|20\\d\\d)\\)$"
        ],
        "MaxDays": 7,
        "ImageBasePath": "../Site/wwwroot/images/cache",
